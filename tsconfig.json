{
  "extends": "./config/tsconfig.client.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["/*"],
      "@apis/*": ["apis/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@providers/*": ["providers/*"],
      "@styles/*": ["styles/*"],
      "@pages/*": ["pages/*"],
      "@typings/*": ["typings/*"],
      "@utils/*": ["utils/*"],
      "@hooks/*": ["hooks/*"]
    }
  },
  "include": ["src", "tmp"]
}
